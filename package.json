{
  "name": "react-base",
  "version": "0.0.1",
  "description": "a base project for react",
  "scripts": {
    "clean": "rimraf ./public/assets/*",
    "build:webpack": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "start": "NODE_ENV=development npm run clean && npm run dev",
    "dev": "BABEL_ENV=development NODE_ENV=development node devServer.js",
    "lint": "eslint web",
    "gen": "node web/utils/gen"
  },
  "keywords": [
    "react",
    "redux",
    "react-router",
    "babel",
    "webpack",
    "sass"
  ],
  "repository": {
    "type": "git"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^3.1.9",
    "babel-preset-react-hmre": "^1.0.1",
    "eslint": "^1.3.1",
    "eslint-plugin-react": "^2.3.0",
    "express": "^4.13.3",
    "react-transform-catch-errors": "^1.0.0",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0"
  },
  "dependencies": {
    "autoprefixer": "^6.3.1",
    "axios": "^0.9.1",
    "babel-core": "^6.3.26",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "classnames": "^2.2.3",
    "css-loader": "^0.23.0",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.5",
