    "moment": "^2.10.6",
    "node-sass": "^3.4.2",
    "normalize.css": "^3.0.3",
    "postcss-loader": "^0.8.0",
    "react": "0.14.7",
    "react-dom": "0.14.7",
    "react-redux": "^4.4.0",
    "react-router": "^2.0.0",
    "react-router-redux": "^4.0.0",
    "redux": "^3.3.1",
    "redux-actions": "^0.9.1",
    "redux-promise": "^0.5.0",
    "redux-thunk": "^2.0.1",
    "rimraf": "^2.4.3",
